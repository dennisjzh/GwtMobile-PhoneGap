<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='KitchenSink'>
	<inherits name="com.google.gwt.user.User" />
    <inherits name='com.google.gwt.json.JSON'/>	

	<!-- Other module inherits                                -->
	<inherits name='com.gwtmobile.phonegap.gwtmobile_phonegap' />
	<inherits name='com.gwtmobile.ui.gwtmobile_ui' />

	<source path="client" />

	<stylesheet src="kitchensink.css"/>

	<entry-point class="com.gwtmobile.phonegap.kitchensink.client.KitchenSink" />
	
	<set-property name="user.agent" value="safari"/>

</module>
